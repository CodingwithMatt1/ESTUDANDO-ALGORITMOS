algoritmo "somador"
var
   cont, N, S, maior, menor: inteiro
inicio
   cont <- 1
   S <- 0
    escreva ("Digite o", cont, "o. valor:")
      leia(N)
   maior <- N
   menor <- N
   S <- S + N
   cont <- cont + 1
   
   enquanto (cont <= 5) faca
      escreva ("Digite o", cont, "o. valor:")
      leia(N)
      se (N > maior) entao
         maior <- N
      fimse
      se (N < menor) entao
         menor <- N
      fimse
      S <- S + N
      cont <- cont + 1
   fimenquanto
   escreval("RESULTADO DA SOMA:",S)
   escreval("O MAIOR NÚMERO FOI", maior)
   escreval("O MENOR NÚMERO FOI", menor)


fimalgoritmo
